{
  "name": "fp-theme",
  "dark": true,
  "author": "Josh Davies",
  "editorScheme": "./fp_theme.xml",
  "colors": {
    "defBackground": "#3D3051",
    "midBackground": "#261F34",
    "darkBackground" : "#1C1C1C",
    "lightBackground": "#B36AE2",
