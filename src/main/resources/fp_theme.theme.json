      "buttonColor":  "darkGrey",
      "borderColor": "darkGrey"
    },

    "ToolTip": {
      "background": "defBackground",
      "Actions.background": "#281A33"
    },

    "ToolWindow": {
      "Header": {
        "background": "#4b335c",
        "inactiveBackground": "#2a1a33"
      },

      "HeaderTab": {
        "selectedBackground": "#4e3a57",
        "selectedInactiveBackground": "defBackground",
        "hoverBackground": "#4e3a57",
        "hoverInactiveBackground": "#4e3a57",

        "underlineColor": "#694f92ff",
        "inactiveUnderlineColor": "#694f92ff",
        "underlineHeight": 5
      },

      "Button": {
        "hoverBackground": "#563b63",
        "selectedBackground": "#563b63",
        "selectedForeground": "offWhite"
      }
    },


    "Tree.modifiedItemForeground": "lightBackground",

    "ValidationTooltip": {
      "errorBackground": "#E6194B",
      "errorBorderColor": "#EB4C5D",
      "warningBackground": "#c17b5c",
      "warningBorderColor": "#bb8f73"
    },

    "VersionControl": {
      "Log.Commit": {
        "currentBranchBackground": "midBackground",
        "unmatchedForeground": "defBackground"
      },
      "RefLabel": {
        "backgroundBase": "midBackground",
        "backgroundBrightness": 0.7
      },
      "FileHistory.Commit.selectedBranchBackground": "midBackground"
    },

    "WelcomeScreen": {
      "Projects.selectionInactiveBackground": "defBackground",
      "separatorColor": "darkGrey"
    },

    "Window.border" : "1,1,1,1,darkBackground"
