      "hoverThumbColor": "#694f92ff",
      "hoverTrackColor": "#3d2240ff",
      "thumb": "#2f2040ff",
      "thumbBorderColor": "#2c152fff",
      "thumbColor": "#6c499194",
      "thumbDarkShadow": "#261226ff",
      "thumbHighlight": "#4f2a4eff",
      "thumbShadow": "#22162bff",
      "track": "#23182dff",
